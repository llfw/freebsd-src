# le-fay.org pf kernel
# This includes pf and altq with pf default to drop.

include		"LF"
ident		LF-PF

# PF
device		pf
device		pflog
device		pfsync
options		PF_DEFAULT_TO_DROP

# altq
options         ALTQ
options         ALTQ_CBQ
options         ALTQ_CODEL
options         ALTQ_RED
options         ALTQ_RIO
options         ALTQ_HFSC
options         ALTQ_CDNR
options         ALTQ_PRIQ
options         ALTQ_FAIRQ

options		DUMMYNET
